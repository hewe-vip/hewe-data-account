<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="vip.hewe.data.dao.ProfileDao">
    <resultMap id="ProfileBaseResultMap" type="profile">
        <id column="id" jdbcType="VARCHAR" property="id" />
        <result column="sex" jdbcType="TINYINT" property="sex" />
    </resultMap>
    <sql id="Base_Column_List">
        id, sex
    </sql>
    <select id="selectByPrimaryKey" parameterType="java.lang.String"
            resultMap="ProfileBaseResultMap">
        select
        <include refid="Base_Column_List" />
        from profile
        where id = #{id,jdbcType=VARCHAR}
    </select>
    <delete id="deleteByPrimaryKey" parameterType="java.lang.String">
        delete from
        profile
        where id = #{id,jdbcType=VARCHAR}
    </delete>

    <insert id="insert" parameterType="profile">
        insert into profile (id,sex)
        values (#{id,jdbcType=VARCHAR},#{sex,jdbcType=TINYINT})
    </insert>
    <update id="updateByPrimaryKey" parameterType="profile">
        update profile
        set
        sex = #{sex,jdbcType=TINYINT}
        where id = #{id,jdbcType=VARCHAR}
    </update>
</mapper>